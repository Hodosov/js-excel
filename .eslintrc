{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "require-jsdoc": "off",
        "comma-dangle": "off",
        "indent": "off",
        "object-curly-spacing": "off",
        "operator-linebreak": "off",
        "no-trailing-spaces": "off",
        "quotes": "off"
    },
    "extends": [
        "eslint:recommended", "google"
    ]
}